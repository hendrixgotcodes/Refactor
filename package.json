    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^5.0.1",
    "figlet": "^1.5.2",
    "gradient-string": "^2.0.1",
    "inquirer": "^8.2.4",
    "nanospinner": "^1.1.0",
    "ora": "^6.1.0",
    "yargs": "^12.0.2"
